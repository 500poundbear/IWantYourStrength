<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dereknam.strengthapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UpdatePage"
            android:label="@string/title_activity_update_page"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.MainActivity" />
        </activity>
        <activity
            android:name=".EditIndividual"
            android:label="@string/title_activity_edit_individual"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.MainActivity" />
        </activity>
        <activity
            android:name=".Snapshots"
            android:label="@string/title_activity_snapshots"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.MainActivity" />
        </activity>
        <activity
            android:name=".ViewIndividuals"
            android:label="@string/title_activity_view_individuals"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.MainActivity" />
        </activity>
        <activity
            android:name=".NewIndividual"
            android:label="@string/title_activity_new_individual"
            android:parentActivityName=".ViewIndividuals" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.ViewIndividuals" />
        </activity>
        <activity
            android:name=".ViewGroups"
            android:label="@string/title_activity_view_groups"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.MainActivity" />
        </activity>
        <activity
            android:name=".EditGroup"
            android:label="@string/title_activity_edit_group"
            android:parentActivityName=".ViewGroups" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.ViewGroups" />
        </activity>
        <activity
            android:name=".NewGroup"
            android:label="@string/title_activity_new_group"
            android:parentActivityName=".ViewGroups" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dereknam.strengthapp.ViewGroups" />
        </activity>
    </application>

</manifest>
